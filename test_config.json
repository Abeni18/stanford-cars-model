{
    "name": "ResNet18-AutoAugment",
    "n_gpu": 1,

    "arch": {
        "type": "ResNet18",
        "args": {
            "use_pretrained": true,
            "num_classes": 196
        }
    },
    "data_processing": {
        "type": "CarsDataLoader",
        "args":{
            "data_dir": "data_processing/datasets/testing/extracted/",
            "metas": "data_processing/datasets/cars_metas/cars_test_annos_withlabels.mat",
            "resize_width": 512,
            "resize_height": 256,
            "batch_size": 80,
            "shuffle": false,
            "validation_split": 0.0,
            "num_workers": 1,
            "limit": null
        }
    }
}
